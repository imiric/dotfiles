! Xft settings ---------------------------------------------------------------

Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintslight

!-------------------------------------------------------------------------------
!
!   U R X V T - Settings
!
URxvt*geometry:                 100x30
URxvt*borderLess:               false
URxvt.depth:                    32
!urxvt*transparent:              true
!urxvt*shading:                  5
URxvt.background:               rgba:0000/0000/0000/eeee
URxvt*scrollBar:                false
URxvt*font:                     xft:Envy Code R for Powerline-10
URxvt*cursorBlink:              true
!URxvt*cursorUnderline:          true
URxvt*perl-ext-common:          default,clipboard
URxvt.iso14755:                 false
URxvt.iso14755_52:              false
URxvt.keysym.C-S-C:             perl:clipboard:copy
URxvt.keysym.C-S-V:             perl:clipboard:paste
URxvt.clipboard.copycmd:        xclip -i -selection clipboard
URxvt.clipboard.pastecmd:       xclip -o -selection clipboard
XTerm*termName:                 xterm-256color
URxvt*termName:                 xterm-256color
!URxvt*keysym.Home:              \033[7~
!URxvt*keysym.End:               \033[8~
!URxvt.keysym.C-S-M-v:           perl:clipboard:paste_escaped

